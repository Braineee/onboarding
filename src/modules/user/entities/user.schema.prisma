model User {
  id          String     @id @default(uuid())
  name        String
  email       String     @unique
  phoneNumber String     @unique
  password    String
  roleId      String?
  deletedAt   DateTime?
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt

  role        Role?       @relation(fields: [roleId], references: [id])
}